
{
  "manifest_version": 3,
  "name": "Edge AI Daily Dashboard",
  "version": "1.1.0",
  "description": "AI 기반 날씨, 명언, 역사 및 일정 관리 대시보드",
  "permissions": [
    "geolocation",
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "AI Daily Dashboard"
  },
  "host_permissions": [
    "https://*.google.com/"
  ]
}
