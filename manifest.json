
{
  "manifest_version": 2,
  "name": "Edge AI Daily Dashboard",
  "version": "1.0.2",
  "description": "AI 기반 날씨, 명언, 역사 및 일정 관리 대시보드",
  "permissions": [
    "geolocation",
    "storage",
    "https://*.google.com/",
    "https://esm.sh/"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "AI Daily Dashboard"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://esm.sh https://cdnjs.cloudflare.com https://cdn.tailwindcss.com; object-src 'self'"
}
